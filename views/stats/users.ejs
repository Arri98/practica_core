<h1>
Users throught time.
</h1>

<canvas id="usersChart" height=80%   =></canvas>

<input type="hidden" id="graphUsers" value= "<%= users %>">
<input type="hidden" id="graphFechas" value=<%= fechas %>  >


<script type="text/javascript" src="https://cdnjs.cloudflare.com/ajax/libs/Chart.js/2.1.6/Chart.bundle.min.js"></script>

<script>
const fechas=JSON.parse(document.getElementById('graphFechas').value);
const usuarios=JSON.parse(document.getElementById('graphUsers').value);
var ctx = document.getElementById('usersChart').getContext('2d');
var myChart = new Chart(ctx, {
    type: 'line',
    data: {
        labels: fechas,
        datasets: [{
            label: 'Users',
            data: usuarios,
            
        }]
    },
    options: {
        scales: {
            yAxes: [{
                ticks: {
                    beginAtZero: true
                }
            }]
        }
    }
});
</script>